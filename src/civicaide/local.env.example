# OpenAI API Key (required)
# Format: OPENAI_API_KEY=sk-xxx... (your actual API key)
OPENAI_API_KEY=sk-your-api-key-here

# Other optional configuration settings
# MODEL=gpt-4o
# MAX_TOKENS=4096 

# Example environment variables file
# Copy this file to local.env and replace with your actual API keys

# Census API key from https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_api_key_here

# Brave Search API key from https://api.search.brave.com/app/keys
BRAVE_API_KEY=your_brave_api_key_here

# Other configuration
DEBUG=false
LOG_LEVEL=INFO 